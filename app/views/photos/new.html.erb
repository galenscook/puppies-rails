<h1>add photo</h1>

<%= form_for :photo, url: photos_path, html:{class:'col-md-4 col-md-offset-4'} do |f| %>


  <div class='form-group'>
    <%= f.label :url, 'url' %><br>
    <%= f.text_field :url, class:'form-control'  %>
  </div>

  <div class='form-group'>
    <%= f.submit "add photo", class:'btn btn-default'%>
  </div>

<% end %>